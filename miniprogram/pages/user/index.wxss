page {
  background-color: #edece8;
}
.user_btn{
  display:flex; 
  justify-content: center;
  margin-bottom:60px;
  align-items:center;
  background-size: 100% 100%;
}
.user_icon{
  height:40px;
  width:40px;
  top:40px;
  position:relative;
}

.user_content {
  position: relative;
}
.user_content .user_main {
  padding-bottom: 100rpx;
  color: #666;
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translateX(-50%);
  top: -40rpx;
}
.user_content .user_main .orders_wrap {
  background-color: #fff;
  margin-top: 30rpx;
}
.user_content .user_main .orders_wrap .orders_title {
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
.order_fix{
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
.order_exit {
  background-color: #fff;
  padding: 20rpx ;
}


.user_content .user_main .app_info_wrap {
  margin-top: 30rpx;
  background-color: #fff;
}
.user_content .user_main .app_info_wrap .app_info_item {
  padding: 20rpx ;
  border-bottom: 1rpx solid #ccc;
}
.user_content .user_main .app_info_wrap .app_info_contact {
  display: flex;
  justify-content: space-between;
}
.user_content .user_main .recommend_wrap {
  margin-top: 30rpx;
  background-color: #fff;
  padding: 20rpx ;
}
